{
  "specification": [
    {
      "title": "Optionally-blockable content",
      "description": "Test behavior of optionally-blockable content",
      "specification_url": "http://www.w3.org/TR/mixed-content/#category-optionally-blockable",
      "test_expansion": [
        {
          "name": "opt-in-blocks",
          "expansion": "default",
          "source_protocol": "https",
          "source_context": "*",
          "delivery_type": "*",
          "delivery_value": "opt-in",
          "redirection": "*",
          "subresource": {
            "blockable": [],
            "optionally-blockable": "*"
          },
          "origin": ["cross-http", "same-http"],
          "expected": "blocked"
        },
        {
          "name": "no-opt-in-allows",
          "expansion": "default",
          "source_protocol": "https",
          "source_context": "*",
          "delivery_type": "*",
          "delivery_value": null,
          "redirection": "*",
          "subresource": {
            "blockable": [],
            "optionally-blockable": "*"
          },
          "origin": ["cross-http", "same-http"],
          "expected": "allowed"
        }
      ]
    },
    {
      "title": "Blockable content",
      "description": "Test behavior of blockable content.",
      "specification_url": "http://www.w3.org/TR/mixed-content/#category-blockable",
      "test_expansion": [
        {
          "name": "opt-in-blocks",
          "expansion": "default",
          "source_protocol": "https",
          "source_context": "*",
          "delivery_type": "*",
          "delivery_value": "opt-in",
          "redirection": "*",
          "subresource": {
            "blockable": "*",
            "optionally-blockable": []
          },
          "origin": ["cross-http", "same-http"],
          "expected": "blocked"
        },
        {
          "name": "no-opt-in-blocks",
          "expansion": "default",
          "source_protocol": "https",
          "source_context": "*",
          "delivery_type": "*",
          "delivery_value": null,
          "redirection": "*",
          "subresource": {
            "blockable": "*",
            "optionally-blockable": []
          },
          "origin": ["cross-http", "same-http"],
          "expected": "blocked"
        },
        {
          "name": "ws-downgrade-blocks",
          "expansion": "default",
          "source_protocol": "https",
          "source_context": "*",
          "delivery_type": "*",
          "delivery_value": "*",
          "redirection": "*",
          "subresource": {
            "blockable": "websocket-request",
            "optionally-blockable": []
          },
          "origin": ["cross-ws", "same-ws"],
          "expected": "blocked"
        }
      ]
    },
    {
      "title": "Allowed content",
      "description": "Test behavior of allowed content.",
      "specification_url": "http://www.w3.org/TR/mixed-content/",
      "test_expansion": [
        {
          "name": "allowed",
          "expansion": "default",
          "source_protocol": "https",
          "source_context": "*",
          "delivery_type": "*",
          "delivery_value": "*",
          "redirection": ["no-redirect", "keep-scheme"],
          "subresource": {
            "blockable": "*",
            "optionally-blockable": "*"
          },
          "origin": ["same-https"],
          "expected": "allowed"
        },
        {
          "name": "websocket-allowed",
          "expansion": "default",
          "source_protocol": "https",
          "source_context": "*",
          "delivery_type": "*",
          "delivery_value": "*",
          "redirection": ["no-redirect", "keep-scheme"],
          "subresource": {
            "blockable": "websocket-request",
            "optionally-blockable": []
          },
          "origin": ["same-wss"],
          "expected": "allowed"
        }
      ]
    }
  ],

  "excluded_tests": [
    {
